# Força uso de npm (não yarn ou pnpm)
engine-strict=true

# Salva exatas versões no package-lock.json
save-exact=true

# Registry padrão npm
registry=https://registry.npmjs.org/

# Configurações de performance
prefer-offline=true
fetch-retries=3
fetch-retry-mintimeout=10000
fetch-retry-maxtimeout=60000

