# Environment variables (optional overrides)

# Logging
LOG_LEVEL=INFO

# Auto-open report after generation
AUTO_OPEN_REPORT=false

# SerpAPI Configuration (optional - get free key at https://serpapi.com)
# Free tier: 250 searches/month, 50/hour throughput
# If not set, falls back to free scraping (less reliable)
USE_SERPAPI=true
SERPAPI_KEY=your_api_key_here

